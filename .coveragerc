[run]
omit =
    */test_*
    *__init__.py
    */.tox/*
    */.virtualenvs/*
    test/*
    */tests/*

source = 
    examples/
    sysbrokers/
    syscontrol/
    syscore/
    sysdata/
    sysexecution/
    sysinit/
    syslogdiag/
    sysobjects/
    sysproduction/
    systems/

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    raise AssertionError
    if 0:
    if __name__ == .__main__.:
